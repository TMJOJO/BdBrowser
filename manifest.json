{
    "name": "BetterDiscord",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Allows you to use BetterDiscord on web. Some plugins might not be working correctly.",
    "icons": {
        "16": "assets/logo.png",
        "48": "assets/logo.png",
        "128": "assets/logo.png"
    },
    "permissions": [
        "storage",
        "tabs",
        "windows"
    ],
    "page_action": {

    },
    "content_scripts": [
        {
            "matches": [ "https://*.discord.com/*" ],
            "js": [ "dist/backend.js" ]
        }
    ],
    "content_security_policy": "script-src 'self'; object-src 'self';"
}
