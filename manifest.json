{
    "name": "BetterDiscord",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Allows you to use BetterDiscord on web. Some plugins might not be working correctly.",
    "icons":
    {
        "16": "assets/logo.png",
        "48": "assets/logo.png",
        "128": "assets/logo.png"
    },
    "permissions":
    [
    ],
    "content_scripts":
    [
        {
            "matches":
            [
                "*://*.discord.com/*"
            ],
            "js":
            [
                "dist/backend.js"
            ]
        }
    ],
    "web_accessible_resources":
    [
        {
            "resources":
            [
                "dist/betterdiscord.js",
                "dist/frontend.js"
            ],
            "matches":
            [
                "*://discord.com/*"
            ]
        }
    ]
}
